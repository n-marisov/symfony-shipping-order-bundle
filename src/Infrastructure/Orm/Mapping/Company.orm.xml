<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Maris\SymfonyBundle\ShippingOrderBundle\Entity\Company" table="business">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="legalForm" column="legal_form" type="string"/>
        <field name="name" column="legal_name" type="string"/>
        <field name="management" column="management" type="string"/>

    </entity>

</doctrine-mapping>